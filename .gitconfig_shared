[alias]
    st = status
    ss = status -s
    co = checkout
    br = branch
    unstage = reset HEAD --
    cancel = reset --soft HEAD^
    graph = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
[grep]
    lineNumber = true
[color]
    ui = true
[color "grep"]
    match = "#FF0AB3"
    linenumber = "#D0B03C"
    filename = "#ADFF2F"
[pager]
    log = diff-highlight | less
    show = diff-highlight | less
    diff = diff-highlight | less
[pull]
    rebase = false
